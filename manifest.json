{
  "manifest_version": 2,
  "name": "Esti Mate",
  "version": "1.0",
  "description": "Your estimate best friend",
  "permissions": [
    "*://api.airtable.com/*",
    "webRequest"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.harvestapp.com/estimates/*/edit"],
      "js": [
        "lib/rx.min.js",
        "src/lib/summary.js",
        "src/lib/airtable.js",
        "src/lib/pm.js",
        "src/App.js",
        "src/index.js"
      ]
    }
  ]

}